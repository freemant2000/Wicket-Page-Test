<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.ttdev</groupId>
	<artifactId>wicket-page-test</artifactId>
	<packaging>pom</packaging>
	<version>3.0.0</version>
	<name>wicket-page-test</name>
	<url>http://wicketpagetest.sourceforge.net</url>
	<properties>
		<wpt.version>3.0.0</wpt.version>
		<wicket.version>6.4.0</wicket.version>
		<slf4j.version>1.6.2</slf4j.version>
	</properties>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
					<verbose>true</verbose>
				</configuration>
			</plugin>
			<plugin>
				<groupId>com.mycila.maven-license-plugin</groupId>
				<artifactId>maven-license-plugin</artifactId>
				<configuration>
					<header>src/etc/header.txt</header>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-site-plugin</artifactId>
				<version>2.2</version>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-project-info-reports-plugin</artifactId>
				<configuration>
					<dependencyDetailsEnabled>false</dependencyDetailsEnabled>
					<dependencyLocationsEnabled>false</dependencyLocationsEnabled>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<reporting>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-project-info-reports-plugin</artifactId>
				<reportSets>
					<reportSet>
						<reports>
							<report>project-team</report>
							<report>issue-tracking</report>
							<report>license</report>
							<report>scm</report>
						</reports>
					</reportSet>
				</reportSets>
			</plugin>
		</plugins>
	</reporting>
	<modules>
		<module>wpt-core</module>
		<module>wpt-runtime</module>
		<module>wpt-runtime-spring</module>
		<module>wpt-runtime-guice</module>
		<module>wpt-sample-spring</module>
		<module>wpt-sample-junit</module>
		<module>wpt-sample-plain</module>
		<module>wpt-sample-guice</module>
	</modules>
	<repositories>
		<repository>
			<id>openqa</id>
			<name>OpenQA Repository</name>
			<url>http://nexus.openqa.org/content/repositories/releases/</url>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
			<releases>
				<enabled>true</enabled>
			</releases>
		</repository>
	</repositories>
	<licenses>
		<license>
			<name>LGPL 3</name>
			<url>http://www.opensource.org/licenses/lgpl-3.0.html</url>
			<distribution>repo</distribution>
		</license>
	</licenses>
	<developers>
		<developer>
			<id>freemant</id>
			<name>Kent Tong</name>
			<email>freemant2000@yahoo.com</email>
			<url>http://agileskills2.org</url>
		</developer>
		<developer>
			<id>andychu927</id>
			<name>Andy Chu</name>
			<email>andychu@gmail.com</email>
			<url>http://www.pisoft.com.mo</url>
		</developer>
	</developers>
	<scm>
		<connection>scm:svn:https://wicketpagetest.svn.sourceforge.net/svnroot/wicketpagetest</connection>
		<developerConnection>scm:svn:https://wicketpagetest.svn.sourceforge.net/svnroot/wicketpagetest</developerConnection>
		<url>http://wicketpagetest.svn.sourceforge.net/viewvc/wicketpagetest</url>
	</scm>
	<inceptionYear>2009</inceptionYear>
	<issueManagement>
		<system>sourceforge</system>
		<url>https://sourceforge.net/tracker/?group_id=287085&amp;atid=1216366</url>
	</issueManagement>
	<distributionManagement>
		<repository>
			<id>wpt-release</id>
			<url>file:/home/kent/oss/mirror/wpt/htdocs/m2-repo/releases</url>
			<uniqueVersion>false</uniqueVersion>
		</repository>
		<site>
			<id>wpt-site</id>
			<url>file:/home/kent/oss/mirror/wpt/htdocs</url>
		</site>
	</distributionManagement>
</project>